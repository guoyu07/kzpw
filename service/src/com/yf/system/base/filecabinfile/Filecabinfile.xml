     <result property="createuser" column="C_CREATEUSER"/>
  
     <result property="createtime" column="C_CREATETIME"/>
  
     <result property="modifyuser" column="C_MODIFYUSER"/>
  
     <result property="modifytime" column="C_MODIFYTIME"/>
  
