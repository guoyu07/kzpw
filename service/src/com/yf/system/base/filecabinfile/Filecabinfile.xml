	  					C_CREATETIME = #createtime#,
	  					C_MODIFYUSER = #modifyuser#,
	  					C_MODIFYTIME = #modifytime#,
	  					C_RIGHT = #right#,
	  					C_FILEPATH = #filepath#,
	  					C_FILESIZE = #filesize:DECIMAL#,
						C_CREATEID = #craeteid:DECIMAL#
  						 
    
    where ID = #id#
  </update>
  
 
