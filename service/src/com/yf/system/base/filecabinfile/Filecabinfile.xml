		</isNotEmpty>
	
	</dynamic>
     <dynamic prepend="">
		<isNotEmpty prepend="" property="orderby">
			$orderby$
		</isNotEmpty>
	
	</dynamic>
	
  </select>
  
   <select id="findAllFilecabinfileBySql"  resultMap="resultFilecabinfileMap"  parameterClass="java.util.Map">  
			$sql$
   </select>
   
   
   <update id="excuteFilecabinfileBySql"   parameterClass="java.util.Map">  
			$sql$
   </update>
  
